<?xml version="1.0"?>
<typesystem package="QtMobility.Bearer">
    <load-typesystem name="typesystem_core.xml" generate="no"/>
    <load-typesystem name="typesystem_network.xml" generate="no"/>

    <enum-type name="QtMobility::QNetworkConfigurationManager::Capability" flags="QtMobility::QNetworkConfigurationManager::Capabilities"/>
    <enum-type name="QtMobility::QNetworkConfiguration::Type"/>
    <enum-type name="QtMobility::QNetworkConfiguration::Purpose"/>
    <enum-type name="QtMobility::QNetworkConfiguration::StateFlag" flags="QtMobility::QNetworkConfiguration::StateFlags"/>
    <enum-type name="QtMobility::QNetworkSession::State"/>
    <enum-type name="QtMobility::QNetworkSession::SessionError"/>

    <namespace-type name="QtMobility" generate="yes"/>

    <object-type name="QtMobility::QNetworkConfigurationManager"/>
    <value-type name="QtMobility::QNetworkConfiguration"/>
    <object-type name="QtMobility::QNetworkSession"/>
</typesystem>
